{
  "name": "aj-marketplace",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "migrate": "npx prisma migrate dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma:generate": "NODE_OPTIONS=--max_old_space_size=4096 npx prisma generate",
    "build": "rm -rf ./dist && tsc && tsc-alias",
    "build:prod": "NODE_OPTIONS=--max_old_space_size=4096 npx prisma generate && rimraf ./dist && tsc && npm prune --omit=dev",
    "start": "npx prisma migrate deploy && node dist/server.js",
    "dev": "ts-node-dev --poll src/server.ts",
    "dev:watch": "nodemon --watch src --exec ts-node src/server.ts",
    "cron:test": "jest src/jobs/__tests__/",
    "postinstall": "npm rebuild sharp --force --platform=linux --arch=x64 --libc=glibc && prisma generate"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.19",
